auto lo
iface lo inet loopback
    address 10.0.0.111/32

auto swp1
iface swp1
     address 192.168.11.1/24

auto swp2
iface swp2
     address 192.168.12.1/24

auto mgmt
iface mgmt
    vrf-table auto
    address 127.0.0.1/8

auto eth0
iface eth0 inet dhcp
    vrf mgmt
    post-up sysctl -w net.ipv6.conf.eth0.accept_ra=2
